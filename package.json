{
  "name": "my-lib-react",
  "private": true,
  "version": "1.4.1",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && tsc && vite build",
    "build:patch": "yarn build && yarn version --patch",
    "build:minor": "yarn build && yarn version --minor",
    "build:major": "yarn build && yarn version --major",
    "preview": "vite preview",
    "start": "start-storybook -p 6006",
    "typecheck": "tsc --noEmit",
    "build-sb": "build-storybook -o build/storybook --quiet"
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.12.0 < 19.0.0",
    "react-dom": ">=16.12.0 < 19.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.125",
    "@mui/material": "^5.16.6",
    "@mui/styled-engine-sc": "^5.10.6",
    "@mui/styles": "^5.10.10",
    "@mui/x-date-pickers": "^7.12.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.12",
    "final-form": "^4.20.9",
    "luxon": "^3.3.0",
    "notistack": "^2.0.8",
    "react-datepicker": "^4.11.0",
    "react-final-form": "^6.5.9",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-modal-hook": "^3.0.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/cli": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/luxon": "^3.2.0",
    "@types/react": "^18.0.28",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  }
}
